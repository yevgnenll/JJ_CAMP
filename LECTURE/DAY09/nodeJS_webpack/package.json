    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.0",
    "jquery": "^3.1.1",
    "jquery-easing": "0.0.1",
    "node-sass": "^3.13.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3"
  }
